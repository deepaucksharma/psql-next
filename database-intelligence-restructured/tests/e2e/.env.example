# Example environment file for E2E tests
# Copy this to .env and fill in your actual values

# New Relic Configuration
NEW_RELIC_LICENSE_KEY=your-license-key-here
NEW_RELIC_USER_KEY=your-user-key-here
NEW_RELIC_ACCOUNT_ID=your-account-id-here
NEW_RELIC_API_KEY=${NEW_RELIC_USER_KEY}  # Usually the same as user key
NEW_RELIC_OTLP_ENDPOINT=otlp.nr-data.net:4317

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=testdb

# MySQL Configuration
MYSQL_HOST=localhost
MYSQL_PORT=3306
MYSQL_USER=root
MYSQL_PASSWORD=root
MYSQL_DB=testdb
MYSQL_ENABLED=true

# Collector Configuration
COLLECTOR_ENDPOINT=localhost:4317
METRICS_ENDPOINT=localhost:8889
HEALTH_ENDPOINT=localhost:13133

# Test Configuration
TEST_ENV=local
COVERAGE_ENABLED=false
KEEP_INFRASTRUCTURE=false
REBUILD_COLLECTOR=false