# MySQL Configuration
MYSQL_PRIMARY_ENDPOINT=mysql-primary.default.svc.cluster.local:3306
MYSQL_REPLICA_ENDPOINT=mysql-replica-1.default.svc.cluster.local:3306
MYSQL_USER=otel_monitor
MYSQL_PASSWORD=your_secure_password
MYSQL_DATABASE=
MYSQL_VERSION=8.0
MYSQL_ROLE=primary

# New Relic Configuration
NEW_RELIC_API_KEY=your_new_relic_api_key
NEW_RELIC_ACCOUNT_ID=your_account_id
NEW_RELIC_OTLP_ENDPOINT=otlp.nr-data.net:4317

# Environment Settings
ENVIRONMENT=production
NAMESPACE=monitoring
TEAM_NAME=database-team
SERVICE_NAME=mysql-monitoring
COST_CENTER=engineering

# Cloud Provider Settings
CLOUD_PROVIDER=aws
CLOUD_REGION=us-east-1

# Collector Settings
GOMAXPROCS=2
GOMEMLIMIT=1750MiB